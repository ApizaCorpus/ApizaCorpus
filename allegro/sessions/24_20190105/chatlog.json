        },
        {
            "client_msg_id": "9aff2770-9ae4-41e1-8519-7d88bb6a31f9",
            "type": "message",
            "text": "ALLEGRO_EVENT_SOURCE *al_get_keyboard_event_source(void)",
            "user": "UB659D0FM",
            "ts": "1546717103.005800"
